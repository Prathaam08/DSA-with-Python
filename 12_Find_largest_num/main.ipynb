    "def largest_num(arr):\n",
    "    if not arr:\n",
    "        return None\n",
    "    first = arr[0]\n",
    "    for num in arr:\n",
    "        if num > first:\n",
    "            first = num \n",
    "    return first\n",
    "print(largest_num([12,33,24,56,32]))\n",
    "\n",
    "# TC : 0(n)\n",
    "# SC : 0(1)"
   ]
  }
