    "def fibonacci(num):\n",
    "    if num<=1:\n",
    "        return num\n",
    "    else:\n",
    "        return fibonacci(num-1) + fibonacci(num-2)\n",
    "    \n",
    "n = 10\n",
    "for i in range(n+1):\n",
    "    print(fibonacci(i) , end=' ')"
   ]
